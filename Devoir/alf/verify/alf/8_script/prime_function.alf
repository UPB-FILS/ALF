#Prime function with error#

function prime (n:int):bool
start
	@var p:bool <- true;

	for i from 2 to sqrt(number:n) go
		if n % i == 0 then
			p <- false;
		end;
	end;

	return p;
end;

@var v : int;

v <- read();

if prime(n:v) then
	write(text : v+" is prime");
else
	write(text : v+" is not prime");
end;
