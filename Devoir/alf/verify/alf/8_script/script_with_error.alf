#Prime number function with error#

function prime:bool (n : int)
start
	@var p : bool <- true;

	for i from 2 to  sqrt (number : n)
		if n % i == 0 then
			p <- false;
		end;
	end;

	return p;
end;

@var v : int;

v <- read();

if prime (n : v) then
	 write (text : v+" is prime");
else
	 write (text : v+" is not prime");
end;